    <legend>Satisfacción del servicio</legend>
    <div class="grid-1-12 clear"></div>
     <!-- director -->
    <div class="grid-6-12">
      <label>Número de servicio: <em class="formee-req">*</em></label>
      <%=f.text_field  :issue_id%>
    </div>
    
    <!-- submit -->
    <div class="grid-6-12">
      <input class="right" type="submit" title="BUSCAR SERVICIO" value="BUSCAR SERVICIO" />
    </div>
  </fieldset>
 <%end%>
</div>